{
  "name": "ibsheet-angular-component",
  "version": "1.0.2",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "build": "ng build ibsheet-angular && yarn copy-package",
    "copy-package": "node scripts/copy-package.cjs",
    "postbuild": "shx cp projects/ibsheet-angular/README.md LICENSE dist/ibsheet-angular/",
    "build:demo": "ng build demo",
    "test": "ng test ibsheet-angular",
    "lint": "ng lint",
    "pack:lib": "cd dist/ibsheet-angular && yarn npm pack",
    "publish:lib": "cd dist/ibsheet-angular && yarn npm publish",
    "release": "standard-version",
    "release:minor": "standard-version --release-as minor",
    "release:major": "standard-version --release-as major",
    "postrelease": "shx cp CHANGELOG.md dist/ibsheet-angular",
    "release:publish": "yarn release && cd dist/ibsheet-angular && npm publish --access public"
  },
  "prettier": {
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^20.0.0",
    "@angular/compiler": "^20.0.0",
    "@angular/core": "^20.0.0",
    "@angular/forms": "^20.0.0",
    "@angular/platform-browser": "^20.0.0",
    "@angular/router": "^20.0.0",
    "@ibsheet/interface": "^1.0.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular/build": "^20.1.3",
    "@angular/cli": "^20.0.5",
    "@angular/compiler-cli": "^20.0.0",
    "@types/jasmine": "~5.1.0",
    "jasmine-core": "~5.7.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "ng-packagr": "^20.1.0",
    "shx": "^0.4.0",
    "standard-version": "^9.5.0",
    "typescript": "~5.8.2"
  }
}
